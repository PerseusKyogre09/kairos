{
  "name": "blockchain-unstop",
  "version": "1.0.0",
  "description": "AI-Powered Blockchain Event Management Platform",
  "main": "index.js",
  "scripts": {
    "test": "npx hardhat test",
    "test:event": "npx hardhat test --grep \"EventContract\"",
    "test:ticket": "npx hardhat test --grep \"TicketNFT\"",
    "test:gas": "REPORT_GAS=true npx hardhat test",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat run scripts/deploy.js",
    "deploy:ticket": "npx hardhat run scripts/deploy-ticket-nft.js",
    "deploy:all": "npm run deploy && npm run deploy:ticket",
    "integration": "npx hardhat run scripts/integration-test.js",
    "node": "npx hardhat node",
    "clean": "npx hardhat clean"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/hardhat-upgrades": "^1.28.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.5",
    "chai": "^4.3.10",
    "ethereum-waffle": "^4.0.10",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.0",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "smart-contracts",
    "events",
    "hackathon",
    "nft"
  ],
  "author": "Blockchain Unstop Team",
  "license": "MIT"
}